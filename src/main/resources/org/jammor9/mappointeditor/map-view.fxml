<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.jammor9.mappointeditor.MapViewController">
    <ScrollPane fx:id="mapViewScrollPane" prefWidth="2000" prefHeight="600" maxHeight="700" maxWidth="1280" pannable="true" onScroll="#zoomPane">
        <ImageView fx:id="mapImageView"/>
    </ScrollPane>
    <Button text="Load Map" onAction="#loadMap"/>
</AnchorPane>
